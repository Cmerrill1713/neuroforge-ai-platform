{
  "mcp": {
    "servers": {
      "mui-mcp": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@mui/mcp@latest"],
        "description": "Official Material-UI documentation and code examples via MCP"
      },
      "docker-mcp": {
        "type": "stdio",
        "command": "python3",
        "args": ["mcp_docker_server.py"],
        "description": "Docker container and image management"
      }
    }
  }
}
